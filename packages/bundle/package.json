{
  "name": "@graphical-scripts/bundle",
  "version": "0.0.0",
  "description": "npm-package-template",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "default": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "lib/"
  ],
  "scripts": {
    "build": "yarn build:ts",
    "build:ts": "tsc",
    "start": "node lib"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "rollup": "^2.34.2",
    "vm2": "^3.9.2"
  },
  "devDependencies": {
    "@graphical-scripts/rollup-plugin-native-fetch": "^0.0.0",
    "@graphical-scripts/rollup-plugin-strip-source-mapping": "^0.0.0",
    "@graphical-scripts/rollup-plugin-wrap-commonjs-entrypoints": "^0.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-replace": "^2.3.4",
    "rollup-plugin-node-polyfills": "^0.2.1"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "license": "MIT"
}
