{
  "name": "@graphical-scripts/core",
  "version": "0.0.0",
  "description": "npm-package-template",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "graphical": "lib/cli.js"
  },
  "files": [
    "app/",
    "lib/",
    "snowpack-cache/"
  ],
  "scripts": {
    "build": "yarn build:ts && yarn build:css",
    "build:ts": "tsc",
    "build:css": "NODE_ENV=production tailwindcss-cli build | postcss | brotli --stdin > app/tailwind.css.br"
  },
  "dependencies": {
    "@graphical-scripts/dark-mode-selector": "^0.0.0",
    "snowpack": "^3.0.0-rc.1",
    "sucrase": "^3.16.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.0.2",
    "brrr": "^1.0.0",
    "copy-dir": "^1.3.0",
    "cssnano": "^4.1.10",
    "postcss": "^8.1.8",
    "postcss-cli": "^8.3.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.0.1",
    "tailwindcss-cli": "^0.1.2"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
