{
  "name": "@start-dev/rollup-plugin-strip-source-mapping",
  "version": "0.0.0",
  "description": "Remove source maps from files before they are processed by rollup",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "default": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/",
    "lib/"
  ],
  "scripts": {
    "build": "tsc"
  },
  "dependencies": {},
  "devDependencies": {
    "rollup": "^2.34.2"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT"
}
